{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": [2, "never"],
    "new-cap": [2, { "capIsNew": false }],
    "comma-dangle": [2, "always-multiline"],
    "no-else-return": 2,
    "no-use-before-define": [2, "nofunc"],
    "quotes": 0,
    "indent": [1, "tab"],
    "key-spacing": [2, { "align": "value" }]
  }
}
